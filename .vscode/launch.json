		{
			"name": "JLink Attach",
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/build/output.elf",
			"svdFile": "${workspaceRoot}/svd/MDR1986BE92.svd",
			"request": "attach",
			"type": "cortex-debug",
			"servertype": "jlink",
			"device": "K1986BE92QI",
			"interface": "jtag",
			"showDevDebugOutput": "raw",
		}
	]
}