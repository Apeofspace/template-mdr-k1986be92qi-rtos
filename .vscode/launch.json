  
{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "JLink Debug",
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/build/output.elf",
			"svdFile": "${workspaceRoot}/svd/MDR1986BE92.svd",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "jlink",
