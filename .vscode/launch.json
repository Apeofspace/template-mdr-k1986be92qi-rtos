{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "JLink Debug",
      "cwd": "${workspaceRoot}",
      "executable": "build/output.elf",
      "svdFile": "svd/MDR1986BE92.svd",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "device": "K1986BE92QI",
      "interface": "jtag",
      "preLaunchTask": "flash-jlink-K1986BE92QI",
      "showDevDebugOutput": "raw"
    },
    {
      "name": "JLink Attach",
      "executable": "build/output.elf",
      "svdFile": "svd/MDR1986BE92.svd",
      "request": "attach",
      "type": "cortex-debug",
      "servertype": "jlink",
      "device": "K1986BE92QI",
      "interface": "jtag",
      "showDevDebugOutput": "raw"
    }
  ]
}

